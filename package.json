{
  "name": "coding-test",
  "version": "0.0.1",
  "description": "coding-test",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "12 || 14 || 16 || 17"
  },
  "scripts": {
    "start:proxy": "export proxy_host=127.0.0.1 proxy_port=33210 && npm start",
    "dev:proxy": "export proxy_host=127.0.0.1 proxy_port=33210 && npm run dev",
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "build:openapi": "node ./dist/openapi-spec > openapi.json",
    "build:sdk": "npm run build && npm run build:openapi && ng-openapi-gen --input openapi.json --output ./sdk",
    "lint": "npm run eslint && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"src/**/*.ts\" ",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild && npm run lint:fix",
    "precoverage": "npm test",
    "coverage": "open coverage/index.html",
    "coverage:ci": "lb-nyc report --reporter=text-lcov | coveralls",
    "test": "npm run lint:fix && lb-nyc --recursive --reporter=lcov npm run mocha",
    "test:ci": "lb-nyc npm run mocha",
    "posttest": "npm run lint",
    "test:dev": "npm run lint:fix && npm run test & npm run notify:test && npm run open:test",
    "test:prd": "npm run rebuild && lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t api .",
    "docker:run": "docker run -p 3000:3000 -d api",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "rebuild": "npm run clean && npm run build",
    "prestart": "npm run rebuild",
    "start": "node --trace-warnings -r source-map-support/register .",
    "dev": "npm run rebuild && tsc-watch --target es2017 --outDir ./dist --onSuccess \"node .\" ",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "prepare": "husky install",
    "mocha": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "open:api": "open-cli http://[::1]:3000/explorer/",
    "open:test": "opener coverage/lcov-report/index.html",
    "notify:test": "notify -t \"hello skypesky\" -m \"测试已完成\" -s --open http://[::1]:3000/explorer/ --icon ./public/icon.jpg --sound none",
    "notify:api": "notify -t \"hello skypesky\" -m \"我好了\" -s --icon ./public/icon.jpg --sound none"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "skypesky <2565978507@qq.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^4.1.1",
    "@loopback/core": "^3.1.1",
    "@loopback/repository": "^4.1.1",
    "@loopback/rest": "^11.1.1",
    "@loopback/rest-explorer": "^4.1.1",
    "@loopback/service-proxy": "^4.1.1",
    "axios": "^0.26.1",
    "axios-https-proxy-fix": "^0.17.1",
    "cheerio": "^1.0.0-rc.10",
    "cloudflare-bypasser": "^0.0.3",
    "lodash": "^4.17.21",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^8.1.1",
    "@loopback/eslint-config": "^12.0.3",
    "@loopback/testlab": "^4.1.1",
    "@types/axios": "^0.14.0",
    "@types/lodash": "^4.14.179",
    "@types/node": "^12.20.46",
    "eslint": "^8.10.0",
    "mocha": "^9.2.2",
    "source-map-support": "^0.5.21",
    "tsc-watch": "^4.6.2",
    "typescript": "~4.5.5"
  }
}
